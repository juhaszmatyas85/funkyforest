{
  "name": "funkyforest/funkyforest",
  "type": "project",
  "license": "",
  "description": "",
  "autoload": {
    "psr-4": {
      "FunkyForest\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "extra": {
    "installer-paths": {
      "web/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
